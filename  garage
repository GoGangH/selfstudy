@startuml

scale 2
class GarageManager{
    +open()
    +close()
    +stop()
}

class Garage {
    +name: string
    +state: GarageState
    +adress: string
}

class Door {
    -width: int
    -height: int
    -weight: int
    -material: string
    -color: string
    +setColor(string)
}

class Motor {
    -speed: int
    -power: int
    -MAX_WEIGHT: int
    -moter_state: int
    +setSpeed()
    +runMotor(pos)
    +stopMotor()
}

class NotificationLight{
    -state: NotificationLightState
    -Light_Color: BulbColor
    +setState(NotificationLightState)
    +setLight(BulbColor)
}

Enum GarageState{
    Empty
    Full
}

Enum BulbColor{
    GREEN
    YELLOW
    BLUE
}

Enum NotificationLightState{
    Open
    Doing
    Close
}

GarageManager -- Garage
Garage <|-- Door
Garage <|-- Motor
Garage <|-- NotificationLight
Garage --> GarageState

NotificationLight --> BulbColor
NotificationLight --> NotificationLightState


@enduml